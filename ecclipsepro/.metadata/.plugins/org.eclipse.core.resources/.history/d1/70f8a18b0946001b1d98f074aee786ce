package practest;

class quesion_data {
	
	
	
	String question;
    String OptionA;
    String OptionB;
    String OptionC;
    String OptionD;
    String CorrOPT;
    String Explant;
    String images;
    
	quesion_data(String question, String optionA, String optionB, String optionC, String optionD, String corrOPT,
			String explant, String images) {
		super();
		this.question = question;
		OptionA = optionA;
		OptionB = optionB;
		OptionC = optionC;
		OptionD = optionD;
		CorrOPT = corrOPT;
		Explant = explant;
		this.images = images;
	}
	
	
	
    
    
	
	
	

}
