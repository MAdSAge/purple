		for(quesion_data i : qestion) {
			
			if(i.getImages().equals("null")) {
				
			}else {
				String a = i.getImages();
				System.out.println("somth ga");
				Document dot = new Document(a);
				Elements sot = dot.select("a[href]");
				for (Element it:sot) {
				System.out.println(i);}
			}
		} 

	}

}
